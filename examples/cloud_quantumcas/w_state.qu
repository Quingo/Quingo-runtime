import std_ops

operation w_state_3(qs: qubit[3]): unit{
    Ry(qs[0], -1.9106332362490184);
    for(int i = 0; i < 2; i += 1){
        Ry(qs[1], toDouble(-2 * i + 1) * pi / 4.0);
        CNOT(qs[0], qs[1]);
    }
   for(int i = 2; i > 0; i -= 1){
       CNOT(qs[i - 1], qs[i]);
   }
    X(qs[0]);
}

operation main(): unit{
    using(qs: qubit[3]){
        w_state_3(qs);
        for(int i = 0; i < 3; i += 1){
            measure(qs[i]);
        }
    }
}